            </JFXButton>
         </children>
      </HBox>
      <JFXTextField fx:id="projectNameLabel" labelFloat="true" layoutX="78.0" layoutY="39.0" maxWidth="305.0" minWidth="156.0" prefHeight="31.0" prefWidth="305.0" promptText="%project_name_label">
         <font>
            <Font size="16.0" />
         </font>
      </JFXTextField>
      <JFXTextField fx:id="projectMemberNumber" labelFloat="true" layoutX="78.0" layoutY="113.0" maxWidth="305.0" minWidth="305.0" prefHeight="38.0" prefWidth="305.0" promptText="%project_member_number">
         <font>
            <Font size="15.0" />
         </font>
      </JFXTextField>
    </children>

</AnchorPane>
