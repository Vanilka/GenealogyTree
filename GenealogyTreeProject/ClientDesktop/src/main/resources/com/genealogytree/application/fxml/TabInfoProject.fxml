<AnchorPane id="infoProjectTab" fx:id="infoProjectTab" prefWidth="819.0" styleClass="gt_MaintabContentPane" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.genealogytree.application.fxmlcontrollers.TabInfoProjectPaneController">
    <stylesheets>
        <URL value="@../styles/mainstyle.css" />
    </stylesheets>
    <children>
      <HBox fx:id="tabInfoButtonVbox" layoutX="424.0" layoutY="46.0">
         <children>
            <JFXButton fx:id="updateFamilyName" buttonType="RAISED" onAction="#updateFamilyName" prefHeight="74.0" prefWidth="106.0" ripplerFill="#b5f448" text="%update_project_name" textAlignment="CENTER" wrapText="true">
               <styleClass>
                  <String fx:value="jfxButton" />
                  <String fx:value="tabInfoButton" />
               </styleClass>
            </JFXButton>
              <JFXButton fx:id="addSimButton" buttonType="RAISED" onAction="#addNewSim" prefHeight="74.0" prefWidth="106.0" ripplerFill="#b5f448" text="%addMember" textAlignment="CENTER" wrapText="true">
               <styleClass>
                  <String fx:value="jfxButton" />
                  <String fx:value="tabInfoButton" />
               </styleClass>
            </JFXButton>
            <JFXButton fx:id="addRelationButton" buttonType="RAISED" onAction="#addNewRelation" prefHeight="74.0" prefWidth="106.0" ripplerFill="#b5f448" text="%addRelation" textAlignment="CENTER" wrapText="true">
