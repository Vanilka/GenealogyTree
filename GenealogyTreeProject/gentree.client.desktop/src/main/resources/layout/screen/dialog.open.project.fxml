<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTabPane?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<AnchorPane id="newProjectDialog" fx:id="newProjectDialog" styleClass="dialog-pane"
            xmlns="http://javafx.com/javafx/8.0.131" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="gentree.client.desktop.controllers.screen.DialogOpenProjectController">
    <stylesheets>
        <URL value="@/layout/style/gentreestyle.css"/>
    </stylesheets>
    <children>
        <JFXTabPane fx:id="tabPaneOpenProject"
                    styleClass="tabPane"
                    AnchorPane.bottomAnchor="50.0"
                    AnchorPane.leftAnchor="10.0"
                    AnchorPane.rightAnchor="10.0"
                    AnchorPane.topAnchor="10.0"/>
        <JFXButton fx:id="buttonCancel" onAction="#cancel" styleClass="button" text="%cancel" layoutX="74.0"
                   layoutY="209.0" prefHeight="30.0" prefWidth="120.0"/>
        <JFXButton fx:id="buttonConfirm" onAction="#confirm" styleClass="button" text="%create" layoutX="293.0"
                   layoutY="209.0" prefHeight="30.0" prefWidth="120.0"/>
    </children>
</AnchorPane>
