<?xml version="1.0" encoding="UTF-8"?>

<?import gentree.client.visualization.controls.HeaderPane?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<fx:root xmlns:fx="http://javafx.com/fxml/1"
         fx:id="familyGroup"
         styleClass="transparent-dark, pane-padding-30, radius-corner-all"
         type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/8.0.131"
         prefHeight="250" prefWidth="350">
    <stylesheets>
        <URL value="@/layout/style/elementstyle.css"/>
    </stylesheets>
    <children>
        <HeaderPane fx:id="PANEL_HEADER"
                    id="pane-header"
                    styleClass="pane-header"
                    AnchorPane.topAnchor="0"
                    AnchorPane.rightAnchor="0"
                    AnchorPane.leftAnchor="0"/>

        <AnchorPane fx:id="content"
                    id="content"
                    maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                    minWidth="-Infinity"
                    AnchorPane.bottomAnchor="30.0" AnchorPane.leftAnchor="40.0"
                    AnchorPane.rightAnchor="40.0" AnchorPane.topAnchor="50.0">
            <children>
                <HBox fx:id="contentHbox" alignment="CENTER" maxHeight="1.7976931348623157E308"
                      maxWidth="1.7976931348623157E308" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="20.0"
                      AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="30.0"/>
            </children>
        </AnchorPane>
    </children>
</fx:root>
