<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<?import java.net.URL?>
<fx:root fx:id="familyGroup"
         styleClass="transparent-dark, pane-padding-30, radius-corner-all"
         type="javafx.scene.layout.AnchorPane"
         xmlns="http://javafx.com/javafx/8.0.131" xmlns:fx="http://javafx.com/fxml/1"
         prefHeight="250" prefWidth="350">
    <stylesheets>
        <URL value="@/layout/style/elementstyle.css"/>
    </stylesheets>
    <children>
        <AnchorPane fx:id="PANEL_HEADER"
                    id="pane-header"
                    styleClass="pane-header, radius-corner-top"
        AnchorPane.topAnchor="0"
        AnchorPane.rightAnchor="0"
        AnchorPane.leftAnchor="0">
            <children>
                <Label fx:id="nameNode" alignment="CENTER"  layoutY="4.0" text="Label" textFill="WHITE"
                       textOverrun="CENTER_ELLIPSIS" AnchorPane.leftAnchor="35.0" AnchorPane.topAnchor="10.0">
                    <font>
                        <Font size="24.0"/>
                    </font>
                </Label>
            </children>
        </AnchorPane>
        <AnchorPane fx:id="content"
                    id="content"
                    maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                    minWidth="-Infinity"
                    AnchorPane.bottomAnchor="30.0" AnchorPane.leftAnchor="40.0"
                    AnchorPane.rightAnchor="40.0" AnchorPane.topAnchor="50.0">
            <children>
                <HBox fx:id="contentHbox" alignment="CENTER" maxHeight="1.7976931348623157E308"
                      maxWidth="1.7976931348623157E308" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                      AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="30.0"/>
            </children>
        </AnchorPane>
    </children>
</fx:root>
