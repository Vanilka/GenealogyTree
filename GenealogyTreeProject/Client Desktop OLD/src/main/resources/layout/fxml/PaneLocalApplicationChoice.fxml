<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import java.net.URL?>
<Pane xmlns:fx="http://javafx.com/fxml/1" id="AnchorPane" accessibleRole="BUTTON"
      onMouseClicked="#selectLocalApplication"
      prefHeight="400.0" prefWidth="300.0" styleClass="logonFXPane, logonCommon"
      xmlns="http://javafx.com/javafx/8.0.65"
      fx:controller="com.genealogytree.application.fxmlcontrollers.PaneLocalApplicationChoiceController">
    <stylesheets>
        <URL value="@../styles/mainstyle.css"/>
    </stylesheets>
    <children>
        <Label fx:id="localApplicationLabel" layoutX="27.0" layoutY="268.0" prefHeight="100.0" prefWidth="252.0"
               text="%application_choice_local"
               textAlignment="CENTER">
            <font>
                <Font name="Arial Black" size="20.0"/>
            </font>
        </Label>
    </children>
</Pane>
